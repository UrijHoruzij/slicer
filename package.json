{
  "name": "slicer-file",
  "version": "1.0.3",
  "description": "Slicer-File - a library that allows you to upload files in chunks.",
  "scripts": {
    "build": "npm run rm && cross-env NODE_ENV=production babel index.js --out-dir dist",
    "rm": "rimraf ./dist",
    "eslint": "eslint"
  },
  "keywords": [
    "file",
    "slice",
    "upload",
    "thin",
    "FormData",
    "chunk",
    "form",
    "post",
    "multipart",
    "form-data",
    "formdata",
    "large"
  ],
  "author": "Urij Horuzij",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/UrijHoruzij/slicer-file.git",
    "directory": "UrijHoruzij/slicer-file"
  },
  "homepage": "https://github.com/UrijHoruzij/slicer-file"
}
